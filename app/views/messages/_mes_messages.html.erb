<div class="message-container my-messages" id="message-<%= @message.id %>">
    <div class="my-messages-inside">
      <i class="author">
        <small><%= @message.created_at.strftime("%a %b %e at %l:%M%p") %></small>
      </i>
      <p class="p-white"><%= @message.content %></p>
    </div>
  </div>